<h1>Create User</h1>
<%= form_for User.new do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.label :password_confirmation, "Confirm Password" %><br>
  <%= f.password_field :password_confirmation %><br>
  <%= f.submit "Create User" %>
<% end %>
