<h1>New Restaurant</h1>
<%= form_for Restaurant.new do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br>
  <%= f.label :address %><br>
  <%= f.text_field :address %><br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
  <%= f.label :state %><br>
  <%= f.text_field :state %><br>
  <%= f.submit "Create Restaurant" %>
<% end %>
